<div class="panel">
  <div class="form-inline">

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Codificación</label>
      <input type="text" class="form-control" ng-model="vc.transmittal.codificacion">
      <label for="">{{vc.transmittal.correlativo}}</label>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Formato:</label>
      <select class="form-control" ng-model="vc.transmittal.formato"
      ng-options="formato as formato for formato in vc.formatos">
      </select>
      <button type="button" class="btn btn-default btn-xs">Editar</button>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Cliente:</label>
      <select class="form-control" ng-model="vc.transmittal.clienteid"
      ng-change="vc.cambiarCliente()"
      ng-options="cliente.id as cliente.nombre for cliente in vc.clientes">
      </select>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Tipo de envío:</label>
      <select class="form-control" ng-model="vc.transmittal.tipo_envio"
      ng-options="tipo as tipo for tipo in vc.tipos_envio">
      </select>
      <button type="button" class="btn btn-default btn-xs">Editar</button>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Atención:</label>
      <select class="form-control" ng-model="vc.transmittal.atencion"
      ng-change="vc.cambiarContacto()">
        <option ng-repeat="contacto in vc.contactos" value="{{contacto.contactoid}}">
          {{contacto.nombre}}
        </option>
      </select>
      <button type="button" class="btn btn-default btn-xs" ng-click="vc.modalContacto()">Editar</button>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">CD:</label>
      <select class="form-control" ng-model="vc.transmittal.control_documentario"
      ng-options="integrante.uid as integrante.nombre for integrante in vc.control_documentario">
      </select>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Área:</label>
      <input type="text" class="form-control"
      ng-model="vc.datos_contacto_seleccionado.area">
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Días de alerta:</label>
      <input type="text" class="form-control" ng-model="vc.transmittal.dias_alerta">
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Correo:</label>
      <input type="text" class="form-control"
      ng-model="vc.datos_contacto_seleccionado.correo">
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Tipo de proyecto:</label>
      <select class="form-control" ng-model="vc.transmittal.tipo_proyecto"
      ng-options="tipo.tipo_proyecto as tipo.tipo_proyecto for tipo in vc.tipos_proyecto">
      </select>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Logo:</label>
      <input type="text" class="form-control">
      <button type="button" class="btn btn-default btn-xs" ng-click="vc.modalLogo()">Editar</button>
    </section>

    <section class="col-md-12 col-sm-12 form-group panel-body">
      <button type="button" class="btn btn-success" ng-click="vc.guardarConfiguracion()">Guardar</button>
      <button type="button" class="btn btn-danger">Cancelar</button>
    </section>

  </div>
</div>

<!--div de modal edicion de contactos-->
<div class="modal fade" id="modalcontacto" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog modal-sm">
      <div class="modal-content">
        <header class="modal-header">
          <button type="button" class="close" aria-hidden="true">x</button>
          <h4 class="modal-title">Editar Contacto:</h1>
        </header>
          <div class="modal-body">
            <section class="form-group">
              <label for="">Atención:</label>
              <input type="text" class="form-control">
            </section>
            <section class="form-group">
              <label for="">Área:</label>
              <input type="text" class="form-control">
            </section>
            <section class="form-group">
              <label for="">Correo:</label>
              <input type="text" class="form-control">
            </section>
          </div>
          <footer class="modal-footer">
            <button type="button" class="btn btn-info btn-xs">Agregar</button>
            <button type="button" class="btn btn-warning btn-xs">Modificar</button>
            <button type="button" class="btn btn-danger btn-xs">Eliminar</button>
            <button type="button" class="btn btn-success btn-xs">Guardar</button>
          </footer>
      </div>
  </div>
</div>

<!--div de modal edicion de logo-->
<div class="modal fade" id="modallogo" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog modal-sm">
      <div class="modal-content">
        <header class="modal-header">
          <button type="button" class="close" aria-hidden="true">x</button>
          <h4 class="modal-title">Editar Logo:</h1>
        </header>
          <div class="modal-body">
            <section class="form-group">
              <label for="">Cliente:</label>
              <input type="text" class="form-control">
            </section>
            <section class="form-group">
              <label for="">Logo:</label>
              <input type="text" class="form-control">
            </section>
          </div>
          <footer class="modal-footer">
            <button type="button" class="btn btn-info btn-xs">Agregar</button>
            <button type="button" class="btn btn-warning btn-xs">Modificar</button>
            <button type="button" class="btn btn-danger btn-xs">Eliminar</button>
            <button type="button" class="btn btn-success btn-xs">Guardar</button>
          </footer>
      </div>
  </div>
</div>
