<div class="panel">
  <div class="form-inline">

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Codificación</label>
      <input type="text" class="form-control" ng-model="CD.transmittal.codificacion"
      ng-change="CD.cambiarCodificacion()">
      <label for="">{{correlativo}}</label>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Formato:</label>
      <select class="form-control" ng-model="CD.formato_seleccionado"
      ng-options="formato as formato for formato in CD.formatos">
      </select>
      <button type="button" class="btn btn-default btn-sm">Editar</button>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Cliente:</label>
      <select class="form-control" ng-model="CD.proyecto.clienteid"
      ng-change="CD.cambiarCliente()"
      ng-options="cliente.id as cliente.nombre for cliente in CD.clientes">
      </select>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Tipo de envío:</label>
      <select class="form-control" ng-model="CD.tipo_seleccionado"
      ng-options="tipo as tipo for tipo in CD.tipos_envio">
      </select>
      <button type="button" class="btn btn-default btn-sm">Editar</button>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Atención:</label>
      <select class="form-control" ng-model="CD.contacto_seleccionado"
      ng-change="CD.cambiarContacto()">
        <option ng-repeat="contacto in CD.contactos" value="{{contacto.contactoid}}">
          {{contacto.nombre}}
        </option>
      </select>
      <button type="button" class="btn btn-default btn-sm">Editar</button>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">CD:</label>
      <select class="form-control" ng-model="CD.proyecto.control_documentario"
      ng-options="integrante.nombre as integrante.nombre for integrante in CD.control_documentario">
      </select>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Área:</label>
      <input type="text" class="form-control"
      ng-model="CD.datos_contacto_seleccionado.area">
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Días de alerta:</label>
      <input type="text" class="form-control" ng-model="CD.transmittal.dias_alerta">
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Correo:</label>
      <input type="text" class="form-control"
      ng-model="CD.datos_contacto_seleccionado.correo">
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Tipo de proyecto:</label>
      <select class="form-control" ng-model="CD.proyecto.tipo_proyecto"
      ng-options="tipo.tipo_proyecto as tipo.tipo_proyecto for tipo in CD.tipos_proyecto">
      </select>
    </section>

    <section class="col-md-6 col-sm-12 form-group panel-body">
      <label for="" class="col-md-3">Logo:</label>
      <input type="text" class="form-control">
      <button type="button" class="btn btn-default btn-sm">Editar</button>
    </section>

    <section class="col-md-12 col-sm-12 form-group panel-body">
      <button type="button" class="btn btn-success" ng-click="CD.guardarConfiguracion()">Guardar</button>
      <button type="button" class="btn btn-danger">Cancelar</button>
    </section>

  </div>
</div>
