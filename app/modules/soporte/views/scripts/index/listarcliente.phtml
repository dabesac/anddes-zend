    <th style="width: 5%;">TIPO DE CLIENTE</th>
    <th style="width: 5%;">RUC</th>
    <th style="width: 5%;">SOCIO</th>
  </thead>


  <tr ng-repeat="clientes in vc.cliente">
    <td>
      <span editable-text="clientes.clienteid" e-from="tableform" e-required>
         {{clientes.clienteid}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.nombre_comercial" e-from="tableform" e-required >
        {{clientes.nombre_comercial}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.nombre" e-from="tableform">
        {{clientes.nombre}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.codigoid" e-from="tableform" >
        {{clientes.codigoid}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.fecha_registro" e-from="tableform">
        {{clientes.fecha_registro}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.web" e-from="tableform">
        {{clientes.web}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.direccion" e-from="tableform">
        {{clientes.direccion}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.paisid" e-from="tableform">
        {{clientes.paisid}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.departamentoid" e-from="tableform">
        {{clientes.departamentoid}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.provinciaid" e-from="tableform">
        {{clientes.provinciaid}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.distritoid" e-from="tableform">
        {{clientes.distritoid}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.estado" e-from="tableform">
        {{clientes.estado}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.tag" e-from="tableform">
        {{clientes.tag}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.isproveedor" e-from="tableform">
        {{clientes.isproveedor}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.iscliente" e-from="tableform">
        {{clientes.iscliente}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.abreviatura" e-from="tableform">
        {{clientes.abreviatura}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.tipo_cliente" e-from="tableform">
        {{clientes.tipo_cliente}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.ruc" e-from="tableform">
        {{clientes.ruc}}
      </span>
    </td>
    <td>
      <span editable-text="clientes.issocio" e-from="tableform">
        {{clientes.issocio}}
      </span>
    </td>
  </tr>

</table>
</font>

