
<div class="form-group" class="col-md-12"  >
	<div class="col-md-2" >
    <label>Escoja una revision: </label> 
  	</div>
  	<div class="col-md-2" >
    <select class="form-control" ng-model="CP.revisionE"         
      ng-options="c.revision_entregable for c in CP.entregable ">
    </select> 
  	</div>
  	<div class="col-md-2" >
    <button class="btn btn-success" ng-click="CP.buscaentregables(CP.revisionE)" class="btn btn-primary btn-xs">Busca</button>
  	</div>

	<div class="form-group col-md-6">
	           
		<div ng-show="CP.formVisibilityEntregable" class="col-md-12" >	                
			<div class="col-md-12">
				<div class="col-md-3" >
		            <label> Revision</label>
		        </div>        
		        <div class="col-md-3" >
		                    <input class="form-control" type="text" ng-model="CP.revisionEntregable"></input>
		        </div>
		       
				<div class="col-md-3" >      
					<button class="btn btn-warning" ng-click="CP.GuardarEntregable()">Guardar</button>
				</div>            
				<div class="col-md-3" >      
					<button class="btn btn-danger" ng-click="CP.CancelarEntregable()">Cancelar</button>
				</div>
			</div>
		             
		</div>
		<button class="btn btn-primary" ng-click="CP.ShowFormEntregable()" ng-hide="CP.formVisibilityEntregable">
		    Anadir Entregable
		</button>
	        
	</div>

</div>


<!-- ddddddddddddddddddddddwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwww -->


<table class="table table-bordered table-hover table-condensed">

    <tr style="font-weight: bold"> 
		<td style="width:10%">EDT</td>
		<td>tipo_documento</td>
		<td>disciplina</td>
		<td>Codigo Anddes</td>
		<td>Codigo Cliente</td>

		<td>descripcion</td>
		<td>fecha_a</td>
		<td>fecha_b</td>
		<td>fecha_0</td>
		<td><span ng-show="tableform.$visible">Action</span></td>	
    </tr>

    <tr ng-repeat="lista in CP.listaentregable track by $index">
    	
    	<td>      
	        <span editable-select="lista.edt" e-name="edt" e-form="rowform" e-ng-options="s.codigo as s.nombre for s in CP.edt"  >
	           <!-- {{ lista.edt }}  -->
	       	   {{ CP.showStatus(lista) }}  
	        </span>
     	</td> 
 		<td>      
	        <span editable-select="lista.tipo_documento" e-name="tipo_documento" e-form="rowform" e-ng-options="s.value as s.text for s in CP.tipodocumentoE" >
	       	  {{ CP.showTipodoc(lista) }} 
	       	  <!-- {{ lista.tipo_documento }}  -->
	        </span>
     	</td>

     	 <td>
        	<!-- editable username (text with validation) -->
	        <span editable-text="lista.disciplina" e-name="disciplina" e-form="rowform" onbeforesave="checkEntregable($dataentregable, lista.cod_le)" >
	          {{ lista.disciplina  }}
	        </span>
      	</td>


		<td>
			<span editable-text="lista.codigo_anddes" e-name="codigo_anddes" e-form="rowform" onbeforesave="checkEntregable($dataentregable, lista.cod_le)"  >
				{{ lista.codigo_anddes }}
			</span>
		</td>
		<td  >
			<span editable-text="lista.codigo_cliente" e-name="codigo_cliente" e-form="rowform" onbeforesave="checkEntregable($dataentregable, lista.cod_le)" >
				{{ lista.codigo_cliente }}
			</span>
		</td>
		<td>
			<span editable-text="lista.descripcion_entregable" e-name="descripcion_entregable" e-form="rowform" onbeforesave="checkEntregable($dataentregable, lista.cod_le)" >	
				{{ lista.descripcion_entregable }}
			</span>
		</td>
		<td  >
			<span editable-text="lista.fecha_a" e-name="fecha_a" e-form="rowform" onbeforesave="checkEntregable($dataentregable, lista.cod_le)" >
				{{ lista.fecha_a }}
			</span>
		</td>
		<td  >
			<span editable-text="lista.fecha_b" e-name="fecha_b" e-form="rowform" onbeforesave="checkEntregable($dataentregable, lista.cod_le)"  >
				{{ lista.fecha_b }}
			</span>
		</td>
		<td  >
			<span editable-text="lista.fecha_0" e-name="fecha_0" e-form="rowform" onbeforesave="checkEntregable($dataentregable, lista.cod_le)" >
				{{ lista.fecha_0 }}
			</span>

		</td>
	

        <td style="white-space: nowrap">	        
	        <form editable-form name="rowform" onbeforesave="CP.guardatListaentregable($data, lista.cod_le)" 
	        ng-show="rowform.$visible" class="form-buttons form-inline" 
	        shown="inserted == lista">

	          <button type="submit" ng-disabled="rowform.$waiting" class="btn btn-primary">
	            guardar
	          </button>
	          <button type="button" ng-disabled="rowform.$waiting" ng-click="rowform.$cancel()" class="btn btn-default">
	            cancelar
	          </button>
	        </form>
	        <div class="buttons" ng-show="!rowform.$visible">
	          <button class="btn btn-primary" ng-click="rowform.$show()">editar</button>
	          <button class="btn btn-danger" ng-click="CP.deleteEntregable($index,lista.cod_le)">eliminar</button>
	        </div>  
        </td>

    </tr>
</table>

<button class="btn btn-default" ng-click="CP.addListaEntregable()">Agregar Entregable</button>












